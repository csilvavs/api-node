{
  "name": "project3",
  "version": "0.0.1",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@sap-ux/eslint-plugin-fiori-tools": "^0.4.0",
    "@sap-ux/ui5-middleware-fe-mockserver": "2",
    "@sap/ux-ui5-tooling": "1",
    "@sapui5/types": "~1.135.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@ui5/cli": "^3.0.0",
    "@wdio/cli": "^8.19.0",
    "@wdio/local-runner": "^8.19.0",
    "@wdio/mocha-framework": "^8.19.0",
    "@wdio/spec-reporter": "^8.19.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cli": "^2.0.0",
    "karma-qunit": "^4.2.1",
    "karma-typescript-preprocessor": "^0.4.0",
    "karma-ui5": "^4.1.0",
    "puppeteer": "^24.9.0",
    "start-server-and-test": "^2.0.12",
    "typescript": "^5.1.6",
    "ui5-middleware-simpleproxy": "^3.4.1",
    "ui5-test-runner": "^5.7.1",
    "ui5-tooling-transpile": "^3.3.7",
    "wdio-qunit-service": "^0.1.0",
    "wdio-ui5-service": "^2.0.2"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#vsproject3-display\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#vsproject3-display\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "lint": "eslint ./",
    "serve": "ui5 serve --port 8080",
    "ts-typecheck": "tsc --noEmit",
    "prestart": "npm run ts-typecheck",
    "prebuild": "npm run ts-typecheck",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#vsproject3-display\"",
    "int-test": "fiori run --config ./ui5-mock.yaml --open 'test/integration/opaTests.qunit.html'",
    "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "unit-test": "fiori run --config ./ui5-mock.yaml --open 'test/unit/unitTests.qunit.html'",
    "test:run": "ui5-test-runner --url http://localhost:8080/test/testsuite.qunit.html --port 8085",
    "test": "start-server-and-test serve http://localhost:8080 test:run"
  },
  "sapuxLayer": "CUSTOMER_BASE",
  "dependencies": {
    "@openui5/sap.m": "^1.136.0",
    "@openui5/sap.ui.core": "^1.136.0"
  }
}
